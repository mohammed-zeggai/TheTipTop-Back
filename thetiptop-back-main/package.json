    "node.js",
    "express",
    "jwt",
    "authentication",
    "mongodb"
  ],
  "author": "DSP5_ARCHIWEBO22B_G7",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.9.1",
    "nodemon": "^3.0.1",
    "voucher-code-generator": "^1.3.0"
  },
  "devDependencies": {
    "mocha": "^10.1.0"
  }
}
