{
  "name": "thetiptop-back",
  "version": "1.0.0",
  "description": "The backend of the contest app thetiptop",
  "main": "server.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/_mocha ./test/**/*.js --exit",
    "start": "nodemon server.js"
  },
  "nodemonConfig": {
    "verbose": true,
    "restartable": "rs",
